@charset "UTF-8";

.memberBox {
	float: left;
	width: calc(100% - 230px);
	margin: 10px 0px 0px 10px;
}

.memberBox .memberTitle {
	float: left;
	width: 100%;
	padding: 5px 5px 5px 5px;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	margin-bottom: 5px;
}

.memberBox .memberTitle img {
	float: left;
	margin-right: 10px;
}

.memberBox .memberTitle span {
	float: left;
	font-weight: bold;
	font-size: 12px;
	margin-top: 7px;
}

.memberBox .memberBoxList {
	float: left;
	width: 100%;
	margin-bottom: 10px;
	border: 1px #91b2e0 solid;
}

.memberBox .memberBoxList span {
	float: left;
}

.memberBox .memberBoxList .memberField{
	display: block;
	float: left;
	width: 100%;
	padding: 5px 5px 5px 5px;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	background-color: #afc8e9;
	border-bottom: 1px #91b2e0 solid;
}

.memberBox .memberBoxList .memberField span{
	font-size: 12px;
	font-weight: bold;
	color: #304777;
	text-align: center;
}

.memberBox .memberBoxList .memberField .5field{
	width: 5%;
}

.memberBox .memberBoxList .memberField .10field{
	width: 10%;
}

.memberBox .memberBoxList .memberField .15field{
	width: 15%;
}

.memberBox .memberBoxList .memberField .20field{
	width: 20%;
}

.memberBox .memberBoxList .memberField .25field{
	width: 25%;
}

.memberBox .memberBoxList .memberField .30field{
	width: 30%;
}

.memberBox .memberBoxList .memberList {
	float: left;
	width: 100%;
}

.memberBox .memberBoxList .memberList ul {
	float: left;
	width: 100%;
	margin: 10px 0px 10px 0px;
	padding: 5px 5px 0px 5px;
	list-style: none;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
}

.memberBox .memberBoxList .memberList ul li {
	float: left;
	width: 100%;
	text-align: center;
	margin-bottom: 10px;
	color: #304777;
}

.memberBox .memberBoxList .memberList ul li .memberListItem {
	float: left;
	width: 100%;
}

.memberBox .memberBoxList .memberList ul li .memberListItem .memberListItemGuide {
	float: left;
	width: 100%;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
	box-sizing: border-box;
	padding: 5px 5px 5px 5px;
}

.memberBox .memberBoxList .memberList ul li .memberListItem .memberListItemCell {
	float: left;
}

.memberBox .memberBoxList .memberList ul li .memberListItem .memberListItemCell span {
	width: 100%;
}

.memberBox .memberListButtonBox {
/* 	float: left; */
	width: 100%;
	text-align: center;
/* 	margin-bottom: 10px; */
}

.memberBox .memberListButtonBox .buttonLeftBox {
	float: left;
}

.memberBox .memberListButtonBox .buttonCenterBox {
	text-align: center;
	width: 100%;
}


