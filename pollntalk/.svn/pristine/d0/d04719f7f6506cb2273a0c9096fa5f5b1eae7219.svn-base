.agree 
{
	display: none;
}

.agree+label 
{
	display: inline-block;
	color: #333333;
}

.agree+label span 
{
	float: left;
}

.agree+label:before 
{
	float: left;
	background-image: url('../images/stress_uncheck.jpg');
	content: "";
	display: inline-block;
	width: 16px;
	height: 16px;
	margin-top: 4px;
	margin-right: 5px;
	border: 1px #9d9d9d solid;
	border-radius: 30px;
}

.agree:checked+label:before 
{
	background-image: url('../images/stress_check.jpg');
	background-repeat: no-repeat;
	background-position: center center;
}

.inputcalendar
{
	background-image:url('/app/images/calendar.png');
	background-repeat:no-repeat;
	background-position: 10px;
}

/*
 *	-----------------------------------
 *	1024 이상 화면
 *	-----------------------------------
 */
@media screen and (min-width:1025px) 
{
	.votewritebox 
	{
		width: 1024px;
	}
	
	.votewritefield 
	{
		padding: 5px 10px 0px 15px;
	}
	
	.votewritetablebox 
	{
		width: 100%;
	}
	
	.paybox 
	{
		width: 800px;
	}
	
	.inputcontcheckbox
	{
		width: calc(100% - 365px);
	}
}

/*
 *	-----------------------------------
